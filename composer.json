{
    "name": "bssphp/php-cs-fixer-config",
    "description": "Personal PHP-CS-Fixer Configuration",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "bss",
            "email": "beanstack.systems@gmail.com",
            "homepage": "http://beanstack.in/"        
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^7.0|^8.5.23|^9.0"
    },
    "autoload": {
        "psr-4": {
            "bssphp\\Fixer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bssphp\\Fixer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
